{
  "name": "momo",
  "version": "1.0.0",
  "description": "Experimental C to WebAssembly compiler",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "ava",
    "build": "echo \"Building...\" && rollup -c"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ballercat/momo.git"
  },
  "keywords": [
    "wasm",
    "c",
    "compiler"
  ],
  "author": "Felix Maier",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ballercat/momo/issues"
  },
  "homepage": "https://github.com/ballercat/momo#readme",
  "devDependencies": {
    "ava": "^0.19.1",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.5.2",
    "babel-preset-es2015": "^6.24.1",
    "rollup": "^0.43.0",
    "rollup-plugin-babel": "^2.7.1",
    "rollup-plugin-commonjs": "^8.0.2",
    "rollup-plugin-eslint": "^3.0.0",
    "rollup-plugin-uglify": "^2.0.1",
    "snap-shot": "^2.17.0",
    "uglify-es": "^3.0.17"
  },
  "ava": {
    "files": [
      "spec/*.js",
      "!**/utils.js"
    ],
    "source": [
      "src/*.js",
      "test/*.js"
    ],
    "concurrency": 5,
    "failFast": true,
    "failWithoutAssertions": false,
    "tap": false,
    "powerAssert": false,
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  },
  "dependencies": {}
}
